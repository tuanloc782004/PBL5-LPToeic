CREATE TABLE `books` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `author` varchar(50) NOT NULL,
  `created_at` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `description` text NOT NULL,
  `discount` decimal(4,2) NOT NULL,
  `enabled` bit(1) NOT NULL,
  `isbn` varchar(17) NOT NULL,
  `num_of_pages` bigint NOT NULL,
  `price` decimal(10,2) NOT NULL,
  `quantity` bigint NOT NULL,
  `rating` decimal(3,2) NOT NULL,
  `title` varchar(100) NOT NULL,
  `year_of_publication` int NOT NULL,
  `id_language` bigint NOT NULL,
  `id_publisher` bigint NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `UKkibbepcitr0a3cpk3rfr7nihn` (`isbn`),
  KEY `FKqugh5skmqh8ub5vst348nuyya` (`id_language`),
  KEY `FKix55a38nhgwihjsecwmyvl8qq` (`id_publisher`),
  CONSTRAINT `FKix55a38nhgwihjsecwmyvl8qq` FOREIGN KEY (`id_publisher`) REFERENCES `publishers` (`id`),
